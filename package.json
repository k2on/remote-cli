{
    "name": "remote-shell",
    "version": "1.0.0",
    "description": "A tool for building remote shells.",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "repository": "https://github.com/nix2io/package-template.git",
    "author": "Max Koon <maxk@nix2.io>",
    "license": "MIT",
    "private": false,
    "prettier": "@nix2/prettier-config",
    "scripts": {
        "format": "prettier --write ./src/**",
        "test-format": "prettier --check ./src/**",
        "test": "mocha -r ts-node/register tests/**",
        "lint": "eslint src/**",
        "mk-docs": "typedoc --name \"Documentation\" && touch ./docs/.nojekyll",
        "start": "ts-node src/index.ts",
        "start:dev": "yarn start ./test-cli",
        "generate-schema": "ts-node scripts/generateSchema.ts",
        "publish-pkg": "yarn generate-schema & yarn publish --access=public"
    },
    "devDependencies": {
        "@nix2/prettier-config": "^1.0.2",
        "@types/mocha": "8.0.4",
        "@types/node": "14.14.10",
        "@typescript-eslint/eslint-plugin": "4.9.0",
        "@typescript-eslint/parser": "4.9.0",
        "eslint": "7.14.0",
        "eslint-plugin-jsdoc": "30.7.8",
        "json-schema-to-typescript": "^10.1.4",
        "mocha": "8.2.1",
        "prettier": "2.1.2",
        "ts-node": "9.1.0",
        "typedoc": "0.20.36",
        "typescript": "4.0.5"
    },
    "files": [
        "dist"
    ],
    "dependencies": {
        "ajv": "^8.2.0",
        "asciimo": "^0.3.1",
        "ora": "^5.4.0"
    },
    "bin": {
        "remote-shell": "./dist/index.js"
    }
}
